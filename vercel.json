{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ],
    "ignore": [
      "backend/*",
      ".env",
      "*.pyc",
      "__pycache__/"
    ]
  }
  